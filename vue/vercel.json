{
  "trailingSlash": true,
  "redirects": [
    { 
      "source": "/", 
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "(?<lang>(de|fr|it|en)),?.*"
        }
      ],
      "destination": "/:lang", 
      "permanent": false 
    },
    { 
      "source": "/", 
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "[a-z]{2},?.*"
        }
      ],
      "destination": "/en",
      "permanent": false 
    }
  ],
  "rewrites": [
    { "source": "/:lang/:hash/", "destination": "/app.html"},
    { "source": "/:lang/:wshash/:hash/", "destination": "/app.html"} 
    ]
  }